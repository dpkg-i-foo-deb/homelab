nodes:
  # Wazuh indexer server nodes
  indexer:
    - name: wazuh.indexer
      ip: {{ wazuh_indexer_host }}

  # Wazuh server nodes
  # Use node_type only with more than one Wazuh manager
  server:
    - name: wazuh.manager
      ip: {{ wazuh_indexer_manager_host }}

  # Wazuh dashboard node
  dashboard:
    - name: wazuh.dashboard
      ip: {{ wazuh_indexer_dashboard_host }}
